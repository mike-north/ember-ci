{
  "name": "ember-ci",
  "version": "0.1.3",
  "description": "Continuious integration & testing goodies for Ember.js (Travis, Circle-CI)",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:js": "eslint ./*.js addon addon-test-support app config lib server test-support tests",
    "start": "ember serve",
    "test": "ember try:each"
  },
  "repository": "https://github.com/mike-north/ember-ci.git",
  "engines": {
    "node": "^4.5 || 6.* || >= 7.*"
  },
  "author": "Mike North <michael.l.north@gmail.com> (http://mike.works)",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.2",
    "ember-cli": "2.18.0",
    "ember-cli-app-version": "^3.1.3",
    "ember-cli-dependency-checker": "^2.1.0",
    "ember-cli-eslint": "^4.2.1",
    "ember-cli-htmlbars": "^2.0.3",
    "ember-cli-htmlbars-inline-precompile": "^1.0.2",
    "ember-cli-inject-live-reload": "^1.4.0",
    "ember-cli-qunit": "^4.2.1",
    "ember-cli-release": "1.0.0-beta.2",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-test-loader": "^2.2.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-resolver": "^4.5.0",
    "ember-source": "~3.2.0",
    "eslint-plugin-ember": "^5.0.0",
    "eslint-plugin-node": "^5.2.1",
    "loader.js": "^4.0.1"
  },
  "keywords": [
    "ember-addon"
  ],
  "dependencies": {
    "ember-cli-babel": "^6.11.0"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
